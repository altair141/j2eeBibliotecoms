<?page title="ModificarUsuarioCuenta" contentType="text/html;charset=UTF-8"?>
<zk>

	<div id="registroUsuarioDiv" width="100%" align="center"><!-- apply="com.foo.view.ctrl.UsuarioCtrl" -->

		<div id="menu" align="right" width="100%">
